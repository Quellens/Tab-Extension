{
  "name": "TabControl",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
"permissions": [
          "tabs",
          "activeTab",
          "<all_urls>"
],
 "background" : {
        "scripts" : ["background.js"]
    }
    
}